{
  "name": "@levicape/spork-iac-stacks",
  "config": {
    "packagemanager": "pnpm",
    "packageexecutor": "corepack pnpm exec",
    "lintpaths": "./src ./canary ./iac"
  },
  "dependencies": {
    "@levicape/fourtwo": "catalog:",
    "@levicape/fourtwo-builders": "catalog:",
    "@levicape/fourtwo-pulumi": "catalog:",
    "@pulumi/aws": "catalog:pulumi",
    "@pulumi/awsx": "catalog:pulumi",
    "@pulumi/docker-build": "catalog:pulumi",
    "@pulumi/pulumi": "catalog:pulumi",
    "yaml": "catalog:core",
    "zod": "catalog:core"
  },
  "devDependencies": {
    "@types/node": "catalog:types",
    "typescript": "catalog:dev"
  },
  "nx": {
    "targets": {
      "nx:compile": {
        "dependsOn": []
      },
      "dx:precommit": {
        "dependsOn": [
          "nx:compile"
        ]
      }
    }
  }
}
