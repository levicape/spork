{
  "name": "iac_stacks",
  "config": {
    "packagemanager": "pnpm",
    "packageexecutor": "pnpm exec"
  },
  "dependencies": {
    "@levicape/fourtwo-pulumi": "catalog:",
    "@pulumi/aws": "catalog:pulumi",
    "@pulumi/awsx": "catalog:pulumi",
    "@pulumi/pulumi": "catalog:pulumi"
  },
  "devDependencies": {
    "@types/node": "catalog:types",
    "typescript": "catalog:dev"
  },
  "nx": {
    "targets": {
      "nx:compile": {
        "dependsOn": [
          "check"
        ]
      },
      "dx:precommit": {
        "dependsOn": [
          "nx:compile",
          "iac_stacks_bootstrap:nx:precommit"
        ]
      }
    }
  },
  "scripts": {
    "check": "$npm_package_config_packageexecutor nx exec -- tsc --noEmit",
    "compile": "$npm_package_config_packageexecutor nx nx:compile"
  }
}
