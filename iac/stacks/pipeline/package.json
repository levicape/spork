{
  "config": {
    "packagemanager": "pnpm",
    "packageexecutor": "pnpm exec"
  },
  "dependencies": {
    "@pulumi/aws": "catalog:pulumi",
    "@pulumi/awsx": "catalog:pulumi",
    "@pulumi/docker-build": "catalog:pulumi",
    "@pulumi/pulumi": "catalog:pulumi"
  },
  "devDependencies": {
    "@types/node": "catalog:types",
    "ci": "catalog:package",
    "publint": "catalog:package",
    "syncpack": "catalog:package",
    "typescript": "catalog:dev"
  },
  "private": true,
  "scripts": {
    "build:ci": "$npm_package_config_packagemanager run clean:install && $npm_package_config_packageexecutor ci && $npm_package_config_packagemanager run generate:ci",
    "clean": "rm -rf $npm_package_config_outpaths || true",
    "clean:install": "echo \"Clean node_modules if npm\"",
    "dx:config:post": "$npm_package_config_packagemanager config set recursive-install true",
    "dx:config:pre": "$npm_package_config_packagemanager config set recursive-install false",
    "dx:lint": "$npm_package_config_packagemanager build:ci && $npm_package_config_packagemanager run package:format",
    "dx:prepublish": "$npm_package_config_packagemanager run dx:config:pre && $npm_package_config_packagemanager run dx:lint && $npm_package_config_packagemanager run dx:config:post",
    "generate:ci": "echo \"Codegen []\"",
    "package:deps": "$npm_package_config_packageexecutor depcheck",
    "package:fix": "$npm_package_config_packageexecutor syncpack fix-mismatches",
    "package:format": "$npm_package_config_packageexecutor syncpack format",
    "package:lockfile": "$npm_package_config_packagemanager install --lockfile-only --lockfile-dir .",
    "package:update": "$npm_package_config_packageexecutor syncpack update",
    "postinstall": "$npm_package_config_packagemanager run package:lockfile"
  },
  "type": "module"
}
