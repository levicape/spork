{
  "name": "@levicape/stands",
  "private": true,
  "type": "module",
  "config": {
    "packagemanager": "pnpm"
  },
  "scripts": {
    "clean:all": "rm -rf ./gen && rm -rf ./node_modules || true",
    "clean:build": "rm -rf ./gen",
    "clean:install": "rm -rf ./node_modules",
    "lint": "npx @bufbuild/buf lint",
    "format": "npx @bufbuild/buf format -w",
    "build:ci": "$npm_package_config_packagemanager run clean:all && npx --yes ci && $npm_package_config_packagemanager run build && $npm_package_config_packagemanager run clean:install",
    "build": "npx --yes @bufbuild/buf generate"
  },
  "dependencies": {
    "typescript": "^5.7.2",
    "@bufbuild/buf": "^1.44.0",
    "ts-proto": "^2.2.2",
    "@bufbuild/protobuf": "^2.2.2"
  },
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf"
}
